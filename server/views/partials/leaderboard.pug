.form-check
  input#isOnLine.form-check-input(type='checkbox', v-model="checked")
  label.form-check-label(for='isOnLine') Online only teams: 
    span(v-text="checked")

table.table(cellspacing="0px", width="100%", style="width: 100%;overflow:hidden;")
  thead
    
    tr
      th(scope="row", v-text="'rank'")
      th Team Name
      th Repo
      th Site
      th Online-only
      th Lighthouse
      th Collaborators
      th Screenshot
      // th(v-for='(key, j) in Object.keys(teams[0])', v-if="['_id', '__v'].indexOf(key) === -1" v-text='key', scope="col")


  tbody(v-if="!checked")
    
    tr(v-for='(team, i) in teams', v-if="team.isOnlineHackathon")
      include team.pug

  tbody(v-else-if="!checked")
    tr(v-for='(team, i) in teams')
      include team.pug
