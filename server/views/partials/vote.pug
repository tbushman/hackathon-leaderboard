td
  
  .form-check.btn-group.btn-group-toggle
    label.btn.btn-primary(v-text="(votes.indexOf(team._id) !== -1 ? 'selected' : '')", v-bind:class="{ active: votes.indexOf(team._id) !== -1 }")
    input(
      type="checkbox",
      v-bind:id="'vote_'+team._id",
      v-model="votes",
      v-bind:value="team._id",
      v-on:click="initValidateVote(team._id)",
      v-bind:checked="votes.indexOf(team._id) !== -1")
